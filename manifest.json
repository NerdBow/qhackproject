{
    "manifest_version": 3,
    "name": "RotOrNot",
    "version": "1.0",
    "permissions": ["tabs", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "icons": {
        "48": "brainRotIcon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "brainRotIcon.png"
        }
    }
}