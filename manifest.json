{
    "manifest_version": 3,
    "name": "RotOrNot",
    "version": "1.0",
    "permissions": ["tabs", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icon.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}